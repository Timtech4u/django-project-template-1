{
  "name": "django-project-template",
  "version": "0.0.1",
  "description": "An opinionated starter kit for a new Django project",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/dansackett/django-project-template",
    "type": "git"
  },
  "scripts": {
    "build-dev": "rm -rf static/bundles/ && webpack --config webpack.config.js --progress",
    "build-prod": "rm -rf static/bundles/ && webpack --config webpack.prod.config.js --progress",
    "watch-dev": "rm -rf static/bundles/ && webpack-dev-server --host 0.0.0.0 --port 45537 --config webpack.dev.config.js --progress --colors",
    "start": "concurrently --kill-others \"python manage.py runserver\" \"yarn watch-dev\""
  },
  "author": "Dan Sackett",
  "license": "MIT",
  "dependencies": {
    "tachyons": "^4.7.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.4.2"
  }
}
